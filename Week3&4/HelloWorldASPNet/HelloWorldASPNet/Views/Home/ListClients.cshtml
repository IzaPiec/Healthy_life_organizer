
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListClients</title>
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
            <th>Age</th>
        </tr>
        @foreach (HelloWorldASPNet.Models.Client client in ViewData["Clients"] as
    List<HelloWorldASPNet.Models.Client>)
        {
            <tr>
                <td>@client.Name</td>
                <td>@client.Age</td>
            </tr>
        }
    </table>
</body>
</html>
